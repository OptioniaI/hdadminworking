local HDAdminGUIS = Instance.new("ScreenGui")
local main = Instance.new("ImageLabel")
local destroy = Instance.new("TextButton")
local trick = Instance.new("ImageLabel")

HDAdminGUIS.Name = "HDAdminGUIS"
HDAdminGUIS.Parent = game.CoreGui
HDAdminGUIS.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

main.Name = "main"
