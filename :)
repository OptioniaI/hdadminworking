trick.Position = UDim2.new(0, 102, 0, -35)
trick.Size = UDim2.new(0, 34, 0, 32)
trick.Visible = true
trick.Active = true
trick.Image = "rbxassetid://7341314260"

destroy.MouseButton1Click:connect(function()
    HDAdminGUIS:Destroy()
end)
