main.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
main.Position = UDim2.new(0, 848, 0, 256)
main.Size = UDim2.new(0, 349, 0, 489)
main.Visible = true
main.Active = true
main.Draggable = true
main.Image = "rbxassetid://5365155154"

destroy.Name = "destroy"
destroy.Parent = main
destroy.BackgroundColor3 = Color3.fromRGB(253, 104, 92)
destroy.Position = UDim2.new(0, 316, 0, 0)
destroy.Size = UDim2.new(0, 33, 0, 34)
destroy.Font = Enum.Font.SourceSans
